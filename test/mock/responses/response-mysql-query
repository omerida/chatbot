            <div class="note" id="37870">  <div class="votes">
                <div id="Vu37870">
                    <a href="/manual/vote-note.php?id=37870&amp;page=function.mysql-query&amp;vote=up" title="Vote up!" class="usernotes-voteu">up</a>
